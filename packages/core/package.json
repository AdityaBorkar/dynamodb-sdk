{
  "name": "dynamodb-sdk",
  "version": "0.0.5",
  "description": "A simple and readable DynamoDB wrapper to streamline and simplify interactions with Amazon DynamoDB.",
  "private": false,
  "keywords": [
    "aws",
    "dynamodb",
    "db",
    "orm",
    "wrapper",
    "document",
    "model",
    "schema",
    "database",
    "nosql",
    "datastore"
  ],
  "homepage": "https://adityaborkar.com/dynamodb-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdityaBorkar/dynamodb-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/AdityaBorkar/dynamodb-sdk/issues",
    "email": "aditya.borkar.programs+npm@gmail.com"
  },
  "author": "Aditya Borkar <aditya.borkar.programs+npm@gmail.com> (https://adityaborkar.com)",
  "license": "MIT",
  "module": "lib/index.ts",
  "type": "module",
  "exclude": [
    "tests"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "docs": "typedoc",
    "dev": "node --watch --import=tsx tests/dev.ts",
    "dev:test": "vitest --watch --disable-console-intercept",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.602.0",
    "@aws-sdk/lib-dynamodb": "^3.602.0",
    "arktype": "2.0.0-dev.24",
    "chalk": "^5.3.0",
    "nanoid": "^5.0.7",
    "sst": "^3.0.33",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "devDependencies": {
    "@arktype/attest": "^0.8.0",
    "@biomejs/biome": "^1.8.2",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "tsx": "^4.15.7",
    "typedoc": "^0.26.1",
    "typescript": "^5.5.2",
    "zod": "^3.23.8"
  }
}